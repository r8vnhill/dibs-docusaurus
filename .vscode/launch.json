{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Docusaurus: pnpm start",
            "runtimeExecutable": "pnpm",
            "runtimeArgs": [
                "start"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Cloudflare Workers: preview",
            "runtimeExecutable": "pnpm",
            "runtimeArgs": [
                "preview"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        }
    ]
}